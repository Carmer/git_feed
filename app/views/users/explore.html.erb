<div class="container">
  <div class="row">
    <h2 class="center-align" id="explore-page" data-user-login="<%= @user.login %>" data-user-token="<%= @user.token %>">Explore Git_Feed</h2>
    <div class="row"K>
    <div class="input-field col s12">
      <input id="explore-username" type="text" class="validate">
      <label for="explore-username">Enter a GitHub username</label>
      <button class="btn btn-small" id="explore-button">Explore</button>
    </div>
  </div>
      <ul class="event-feed-list">
        <li>
        </li>
      </div>
    </ul>
</div>

<div class="container">
  <div class="col s12">
    <div class="windows8 hidden spinner">
      <div class="wBall" id="wBall_1">
        <div class="wInnerBall">
        </div>
      </div>
      <div class="wBall" id="wBall_2">
        <div class="wInnerBall">
        </div>
      </div>
        <div class="wBall" id="wBall_3">
          <div class="wInnerBall">
          </div>
        </div>
        <div class="wBall" id="wBall_4">
          <div class="wInnerBall">
          </div>
        </div>
        <div class="wBall" id="wBall_5">
          <div class="wInnerBall">
        </div>
      </div>
    </div>
  </div>
</div>

<div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
<form action="#">
  <label>Filter by event type:</label>
  <p>
    <input type="checkbox" class="filled-in filter-button CreateEvent" id="filter_check_1" checked="checked" data-event-type="CreateEvent"/>
    <label for="filter_check_1">Create</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button DeleteEvent" id="filter_check_2" checked="checked" data-event-type="DeleteEvent"/>
    <label for="filter_check_2">Delete</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button ForkEvent" id="filter_check_3" checked="checked" data-event-type="ForkEvent"/>
    <label for="filter_check_3">Fork</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button IssuesEvent" id="filter_check_4" checked="checked" data-event-type="IssuesEvent"/>
    <label for="filter_check_4">Issues</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button IssueCommentEvent" id="filter_check_5" checked="checked" data-event-type="IssueCommentEvent"/>
    <label for="filter_check_5">Issue Comment</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button MemberEvent" id="filter_check_6" checked="checked" data-event-type="MemberEvent"/>
    <label for="filter_check_6">Member</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button PushEvent" id="filter_check_7" checked="checked" data-event-type="PushEvent"/>
    <label for="filter_check_7">Push</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in filter-button PullRequestEvent" id="filter_check_8" checked="checked" data-event-type="PullRequestEvent"/>
    <label for="filter_check_8">Pull</label>
  </p>
  <p>
    <input type="checkbox" class="filled-in WatchEvent filter-button" id="filter_check_9" checked="checked" data-event-type="WatchEvent"/>
    <label for="filter_check_9">Watch</label>
  </p>
</form>
  </div>

<div class="container">
  <div class="col s12 center-align load-more-button hidden">
  <button class="load-more-button btn btn-large">Load More Events</button>
  </div>
</div>
